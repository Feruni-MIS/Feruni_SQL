SELECT 
      T1.[SALESQUOTATIONNUMBER]
      ,T1.[SALESQUOTATIONSTATUS]
      ,T1.[SALESQUOTATIONEXPIRYDATE]
      ,T1.[SALESQUOTATIONFOLLOWUPDATE]
      ,T1.[CONTACTPERSONID]
      ,T1.[RECEIPTDATEREQUESTED]
      ,T1.[QUOTATIONRESPONSIBLEPERSONNELNUMBER]
      ,T1.[DEFAULTLEDGERDIMENSIONDISPLAYVALUE]
      ,T1.[PROSPECTID]
      ,T1.[CAMPAIGNID]
      ,T1.[OPPORTUNITYID]
      ,T1.[GENERATEDSALESORDERNUMBER]
      ,T1.[QUOTATIONREASONCODE]
      ,T1.[QUOTATIONTOTALAMOUNT]
      ,T1.[QUOTATIONTOTALCHARGESAMOUNT]
      ,T1.[QUOTATIONTOTALDISCOUNTAMOUNT]
      ,T1.[TOTALDISCOUNTAMOUNT]
      ,T1.[CREATEDDATETIME1]
      ,T1.[DATAAREAID]
      , CONCAT([DATAAREAID], [DEFAULTLEDGERDIMENSIONDISPLAYVALUE]) AS "FinDim Code"
      ,T2.[NAME] AS "Quotation Responsible Personnel Number"
  FROM [dbo].[SalesQuotationHeaderV2Staging] T1
  LEFT JOIN [dbo].[_humanresources_dm_employee] T2
  on T1.[QUOTATIONRESPONSIBLEPERSONNELNUMBER] = T2.[PERSONNELNUMBER]
